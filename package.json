{
  "name": "weather_forecast_app",
  "author": "Will Summers",
  "version": "1.0.0",
  "description": "Basic skeleton codebase for React/Redux/Router applications.",
  "scripts": {
    "preinstall": "npm set audit false",
    "start": "webpack serve --config webpack/dev.config.js",
    "start:docker": "npx webpack serve --config webpack/dev.config.js --host 0.0.0.0 --port 3000",
    "app": "concurrently \"npm run server\" \"npm run client\"",
    "app:local": "sh -c 'pg_ctl -D /usr/local/var/postgres start 2>/dev/null || sudo service postgresql start 2>/dev/null || sudo systemctl start postgresql 2>/dev/null || echo \"PostgreSQL already running or not installed\"; createdb weather_forecast_app_development 2>/dev/null || true; DATABASE_HOST=127.0.0.1 DATABASE_USERNAME=$(whoami) DATABASE_PASSWORD= DATABASE_NAME=weather_forecast_app_development npm run app'",
    "app:docker": "docker compose up --build",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color",
    "server": "rails s -p 3001 -b '0.0.0.0'",
    "client": "npm run start",
    "heroku-postbuild": "npm run build",
    "precommit": "eslint src",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "cache": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.1.0",
    "express": "^5.1.0",
    "extract-loader": "^4.0.3",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss-loader": "^8.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "^4.13.1",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "5.3.4",
    "webpack-dev-server": "5.2.2",
    "webpack-hot-middleware": "2.26.1",
    "webpack-merge": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/register": "^7.27.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^12.1.5",
    "@types/extract-text-webpack-plugin": "^3.0.10",
    "babel-jest": "^29.7.0",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "http-proxy-middleware": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "react-refresh": "^0.18.0",
    "sass": "^1.94.0",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0"
  },
  "overrides": {
    "babel-template": {
      "babel-traverse": "npm:@babel/traverse@latest"
    },
    "babel-helper-function-name": {
      "babel-traverse": "npm:@babel/traverse@latest"
    }
  }
}
